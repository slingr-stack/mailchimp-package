{
  "label": "Add campaign",
  "name": "addCampaignMailchimp",
  "category": "integrations",
  "description": "This flow step will create a new Mailchimp campaign.",
  "inputs": [
    {
      "label": "Type",
      "name": "type",
      "type": "dropDown",
      "description": "There are four types of campaigns you can create in Mailchimp.",
      "required": "true",
      "defaultValue": [
        "regular"
      ],
      "typeOptions": {
        "possibleValues": [
          {
            "label": "regular",
            "name": "regular"
          },
          {
            "label": "plaintext",
            "name": "plaintext"
          },
          {
            "label": "absplit",
            "name": "absplit"
          },
          {
            "label": "rss",
            "name": "rss"
          },
          {
            "label": "variate",
            "name": "variate"
          }
        ]
      }
    },
    {
      "label": "List Id",
      "name": "listId",
      "description": "The unique list id.",
      "type": "text",
      "required": "true",
      "options": {
        "allowContextSelector": "true"
      }
    }
  ],
  "outputs": [
    {
      "label": "Response",
      "name": "response",
      "type": "object",
      "description": "The response to external URL"
    }
  ]
}